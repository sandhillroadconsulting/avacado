# Personal GitHub account (adeebkm)
Host github.com-personal
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

# SandhillRoad Consulting organization
Host github.com-sandhillroad
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_sandhillroad
    IdentitiesOnly yes

# Default GitHub (will use personal)
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes 